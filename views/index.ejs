<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Szinhaz</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
</head>
<body>
  <h1>Színház</h1>
  <table>
    <tr>

      <th>
        Title
      </th>
      <th>Percentage</th>
      <th>Code</th>
    </tr>
    <tr>
      
      <% szinhazdb.forEach((sz) => { %>
        <td href="/szinhazdb/<%= sz.id %>">
          <%= sz.title %>
        </td>
        <% }) %>
        
     
    </td>
    </tr>
  </table>

  
</body>
</html>
